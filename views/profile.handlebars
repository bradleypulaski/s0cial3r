<body id="profile-body">
        <div id="profile-img-container">
            <img src="{{profileImage}}" id="profile-img">     
          
            <h1>{{messages.error}}</h1>
             <h1>{{messages.info}}</h1>
        </div>
        <div class="row" id="profile-page-header">
            <h1 id="profile-header-background-type">s0cial3r</h1>
        </div>
        <nav class="row" id="profile-nav">
            <ul>
            <li> <a href="#" id="profile-logo">{{id}}</a></li>
           <li><span id="username-display">{{displayName}}</span></li>
            <li><form id ="friendhide"method="post" action="/sendfriend/{{id}}"  class="inline">
  <input type="hidden" name="submit_param" value={{id}}>
  <button type="submit" name="submit_param" value="submit_value" class="link-button">
    Add friend
  </button>
</form></li>
            </ul>
            <ul id="nav-mobile" class="right hide-on-med-and-down chat-nav-links">
                 <li></li><a href="/logout" class="waves-effect waves-light btn"><i class="material-icons left">cloud_off</i>Logout</a></li>
                <li><a href="/profile/{{id}}/general" class="nav-links highlighted" id="main-chat-link">Main Chat</a></li>
                <li><a href="/profile/{{id}}/lfg" class="nav-links" id="gaming-chat-link">Gaming Chat</a></li>
                <li><a href="/profile/{{id}}/tv_chat" class="nav-links" id-"tv-chat-link">TV Chat</a></li>
                <li><a href="/profile/{{id}}/movie_chat" class="nav-links" id="movie-chat-link">Movie Chat</a></li>
                <li><a href="/profile/{{id}}/super-bowl-xxx_giggity" class="nav-links" id="sports-chat-link">Sports Chat</a></li>
            </ul>
           

        </nav>
        <div class="row" id="search-bar-div-profile">
            <div class="input-field col m6 offset-m3" id="post-bar-col">
                <input placeholder="Say Something" name="post" type="text" id="post-input"/>

                <input class="btn" type="submit" value="POST" id="post-button"/>
            </div>
        </div>
        <div class="row" id="feed-row">;
        <div class="col m6 offset-m3" id="feed">

        {{#each feed}}
         <div class="card-panel" id="post">
                     <div class="card-content">
                        <a href="#" id="postername">{{username}}</a><span id="says"> says:</span>
                        <br>
                        <br>
                        <span id="post-body">{{message}}</span>
                        <br><br>
                        <a href="#" id="reply">REPLY</a>
                    </div>
                </div>
        {{/each}}

            </div>    
            </div>
        <div class="row" id="profile-page-content">
            <div class="col m3" id="discord-app-col">
                <iframe src="https://discordapp.com/widget?id=406842588239495168&theme=dark" width="350" height="500" allowtransparency="true" frameborder="0" id="discord-widget"></iframe>
            </div>
            <div class="col m6" id="profile-feed-col">

            </div>
            <div class="col m3" id="friends-col">
                <div class="card" id="friends-card">
                    <div class="card-content" id="friends-content">
                        <span class="card-title" id="friends-title">You have <a href="#" id="friends-count"></a> friends</span>
                        <div id="frendo">
                  
                    </div>
                </div>
                
            </div>
            
        </div>
    </div>



